        <template id="portal_my_home_menu_loyalty" name="Portal layout : loyalty menu entries"
                  inherit_id="portal.portal_breadcrumbs" priority="20">
            <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
                <li t-if="page_name == 'loyalty'" class="breadcrumb-item">
                    Loyalty Points
                </li>
            </xpath>
        </template>

        <template
                id="website_loyalty_management_assets_frontend"
                inherit_id="website.assets_frontend"
                name="website_loyalty_management_assets_frontend frontend">
            <xpath expr="." position="inside">
<!--                <script src="/website_loyalty_management/static/src/js/website_loyalty_management.js"-->
<!--                        type="text/javascript"/>-->
                <link type="text/scss" href="/website_loyalty_management/static/src/scss/style.scss" rel="stylesheet"/>

            </xpath>
        </template>

        <template id="my_loyalty" name="My Loyalty">
            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True"/>
                <t t-call="portal.portal_searchbar">
                    <t t-set="title">Loyalty Points</t>
                </t>
                <h3 class="page-header">My Points</h3>
                <div id="your-loyalty-points-tab-content" class="tab-content mb-4">
                    <div class="tab-pane active " id="loyalty_points">
                        <h4>
                            <p class='text-primary'>
                                <b>
                                    <i class="fa fa-calendar-check-o"/>
                                    You have
                                    <t t-esc="env.user.wk_website_loyalty_points"/>
                                    Loyalty Points.
                                </b>
                            </p>
                        </h4>
                    </div>
                </div>

                <t t-if="histories" t-call="portal.portal_table">
                    <thead>
                        <tr class="active">
                            <th>Sale Order No</th>
                            <th class="text-center">Points Process</th>
                            <th class="text-right">Redeemed Date</th>
                            <th class="text-right">Discount Offered</th>
                            <th class="text-right">Points</th>
                        </tr>
                    </thead>
                    <t t-foreach="histories" t-as="history">
                        <tr>
                            <td>
                                <a t-if="history.sale_order_ref" t-att-href="history.sale_order_ref.get_portal_url()">
                                    <t t-esc="history.sale_order_ref.name"/>
                                </a>
                            </td>
                            <td class="text-center">
                                <span t-if="history.loyalty_process == 'addition'" t-esc="'Points Added'"/>
                                <span t-if="history.loyalty_process == 'deduction'" t-esc="'Points Deducted'"/>
                            </td>
                            <td class="text-right">
                                <span t-field="history.date"/>
                            </td>
                            <td class="text-right">
                                <t t-esc="history.redeem_amount"/>
                            </td>
                            <td class="text-right">
                                <span t-field="history.points_processed"/>
                            </td>
                        </tr>
                    </t>
                </t>

            </t>
        </template>

        <template id="portal_my_home_loyalty" name="Portal My Home :Loyalty" inherit_id="portal.portal_my_home"
                  priority="20">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="title">My Rewards</t>
                    <t t-set="url" t-value="'/my/loyalty'"/>
                    <t t-set="placeholder_count" t-value="'points_count'"/>
                    <t t-set="count" t-value="request.env.user.wk_website_loyalty_points"/>
                </t>
                <!-- <div>
                <a href="/my/loyalty" class="list-group-item list-group-item-action">
                  <b>Loyalty Points </b>
                  <span class="badge badge-secondary badge-pill" style="float:right;" t-esc="env.user.wk_website_loyalty_points"/>
                </a>
                </div> -->
            </xpath>
        </template>

         <template id="portal.portal_docs_entry" name="My Portal Docs Entry">
        <a t-att-href="url" t-att-title="title" class="list-group-item list-group-item-action d-flex align-items-center justify-content-between">
            <t t-esc="title"/>
            <t t-if='count'>
                <span class="badge badge-secondary badge-pill" t-esc="count"/>
            </t>
            <t t-elif="placeholder_count">
                <span class="badge badge-secondary badge-pill" t-att-data-placeholder_count="placeholder_count">
<!--                    <i class="fa fa-spin fa-spinner"></i>-->
                    <t t-esc="count"/>

                </span>
            </t>
        </a>
    </template>




