        <template id="message" name="message">
            <div class="_o_redeem_message">
                <t t-set="website_sale_order" t-value="website.sale_get_order()"/>
                <t t-set="wk_website_loyalty_points"
                   t-value="website.format_loyalty_points(env.user.partner_id.wk_website_loyalty_points)"/>
                <t t-set="loyalty_obj" t-value="website.get_active_loyalty_obj(sale_order=website_sale_order)"/>
                <div class="alert alert-danger" t-if="not allowed_redeem">
                    <b>
                        <p t-if="redeem_once">You are allowed to redeem only once per sale order, which you have already
                            redeemed.
                        </p>
                        <p t-if="no_point">Sorry ,You have 0 Points you can't redeem right now !
                        </p>
                        <p t-if="no_order">Your cart is empty !
                        </p>
                        <p t-if="no_loyality_feature">Sorry, Loyalty feature is not available right now, Please try
                            later.
                        </p>
                        <p t-if="all_rule_message">Sorry, you don't have enough points to redeem.
                        </p>
                        <p t-if="no_redeem_rule">No redeem rules are available.
                        </p>
                    </b>
                </div>

                <div t-if="allowed_redeem">
                    <p class="alert alert-success" style="font-size: large;">
                        My Rewards:<t t-esc="wk_website_loyalty_points"/>
                        <br/>
                        <br/>
                        Success!! You got any of the following is free.</p>
                    <t t-foreach="all_rule" t-as="i">
                        <ul>
                            <li>
                                <span style="font-size: 19px;font-family: auto;color: black;"><t t-esc="i.get('points_start')"/> Points will reduce from your account.
                                <br/>
                                </span>
                            </li>
                        <br/>
                        <t t-foreach="i.get('products')" t-as="j">
                            <div style="font-size: 19px;">
                                <input type="radio" t-att-id="i.get('line_id')" t-att-points_end="i.get('points_start')" name="product_loyalty" t-att-value="j.get('product_id')" />
                                <label for="i.get('line_id')">
                                    <t t-esc="j.get('product_name')"/>
                                </label>
                                <br/>

                            </div>


                        </t>
                         <br/>
                        </ul>

                    </t>
                </div>
            </div>
        </template>
        <template id="website_loyalty_management.message_template" name="message_template">
            <div class="modal fade model_confirmation_loyalty"  id="modal_confimation" role="dialog">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="loyalty-banefit-bar"/>
                        <div class="modal-header">
                            <h3 class="mb-0">Loyalty Benefits!</h3>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span class="fa fa-times"></span>
                            </button>
                        </div>
                        <div class="modal-body ">
                            <p class="modal_redeem_rule_sub_label">Thanks for showing interest in our Loyalty Program.
