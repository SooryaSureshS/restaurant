<odoo>
    <data>
        <record id="res_config_settings_view_dinein_table" model="ir.ui.view">
            <field name="name">res.config.settings.view.dinein.table</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="1"/>
            <field name="inherit_id" ref="website.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='website_selection_settings']" position="after">
                    <div class="banner" string="Banner">
                        <h2>POS Table Setting For DineIn</h2>
                        <div class="row mt16 o_settings_container">
                            <div class="col-12 col-lg-6 o_setting_box"
                                 title="Here You can enable or disable table for dine in">
                                <div class="">
                                    <div class="content-group">
                                        <div class="row">
                                            <span class="o_form_label" style="margin-left: 15px;">Table For DineIn</span>
                                            <field name="dine_in_list_table" style="width:20%;margin-left: 15px;"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>